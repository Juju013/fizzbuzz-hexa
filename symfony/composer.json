{
    "name": "leboncoin/fizzbuzz",
    "description": "A project for LeBonCoin technical test.",
    "require": {
        "php": ">= 8.1",
        "symfony/routing": "^6.1",
        "symfony/http-foundation": "^6.1",
        "symfony/http-kernel": "^6.1",
        "symfony/yaml": "^6.1",
        "symfony/config": "^6.1",
        "symfony/framework-bundle": "^6.1",
        "symfony/dotenv": "^6.1",
        "symfony/runtime": "^6.1",
        "symfony/flex": "^2.2",
        "symfony/console": "^6.1",
        "doctrine/doctrine-bundle": "^2.7",
        "symfony/browser-kit": "^6.1"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5",
        "symfony/phpunit-bridge": "^6.1"
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "config": {
        "allow-plugins": {
            "symfony/runtime": true,
            "symfony/flex": true
        }
    },
    "scripts": {
        "auto-scripts": {
            "cache:clear": "symfony-cmd",
            "assets:install %PUBLIC_DIR%": "symfony-cmd"
        }
    }
}
